{
    "name": "daycarealert",
    "version": "0.1.0",
    "private": true,
    "proxy": "http://localhost:8084",
    "dependencies": {
        "@fortawesome/fontawesome-free": "^6.7.2",
        "@react-google-maps/api": "^2.20.6",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^13.4.0",
        "@testing-library/user-event": "^13.5.0",
        "axios": "^1.8.4",
        "bcryptjs": "^3.0.2",
        "bootstrap": "^5.3.3",
        "chart.js": "^3.9.1",
        "cheerio": "^1.0.0",
        "cors": "^2.8.5",
        "csv-parser": "^3.2.0",
        "date-fns": "^2.29.3",
        "dotenv": "^16.4.7",
        "fs-extra": "^11.3.0",
        "helmet": "^8.1.0",
        "html2pdf.js": "^0.10.3",
        "jsonwebtoken": "^9.0.2",
        "lodash": "^4.17.21",
        "moment": "^2.30.1",
        "morgan": "^1.10.0",
        "mysql2": "^3.14.0",
        "node-cron": "^3.0.3",
        "node-fetch": "^2.7.0",
        "nodemailer": "^6.10.1",
        "pdf-parse": "^1.1.1",
        "puppeteer": "^24.4.0",
        "react": "^18.2.0",
        "react-bootstrap": "^2.10.9",
        "react-chartjs-2": "^4.3.1",
        "react-dom": "^18.2.0",
        "react-ga4": "^2.1.0",
        "react-icons": "^5.5.0",
        "react-router-dom": "^6.22.2",
        "react-scripts": "^3.0.1",
        "recharts": "^2.15.1",
        "web-vitals": "^2.1.4",
        "winston": "^3.17.0"
    },
    "scripts": {
        "start": "react-scripts --openssl-legacy-provider start",
        "start-network": "cross-env HOST=0.0.0.0 PORT=8080 react-scripts --openssl-legacy-provider start",
        "start-backend": "cross-env PORT=8081 node backend/server.js",
        "start-simple": "cross-env PORT=8081 node backend/simple-test-server.js",
        "start-fallback": "cross-env PORT=8081 node backend/server-with-fallback.js",
        "start-optimized": "cross-env PORT=8083 node server/optimized-server.js",
        "dev": "concurrently \"npm run start\" \"npm run start-backend\"",
        "dev:simple": "concurrently \"npm run start\" \"npm run start-simple\"",
        "dev:fallback": "concurrently \"npm run start\" \"npm run start-fallback\"",
        "dev:optimized": "concurrently \"npm run start\" \"npm run start-optimized\"",
        "start-with-auth": "bash ./start-with-auth.sh",
        "backend-only": "bash ./run-backend.sh",
        "optimized-only": "bash ./run-optimized-server.sh",
        "full-backend": "concurrently \"npm run backend-only\" \"npm run optimized-only\"",
        "auth-test": "node test-direct-auth.js",
        "build": "react-scripts --openssl-legacy-provider build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "import-noncompliance": "node import-noncompliance-data.js",
        "fetch-data": "node import-batch.js",
        "fetch-simple": "node simple-data-fetcher.js",
        "test-db": "node test-db-connection.js",
        "test-user": "node test-existing-user.js",
        "create-tables": "mysql -p < setup-database.sql"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "concurrently": "^8.2.2",
        "cross-env": "^7.0.3",
        "http-proxy-middleware": "^2.0.6"
    }
}
